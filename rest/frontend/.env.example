# Backend API URL - Change this to your public backend URL
REACT_APP_API_URL=http://your-domain-or-ip:8000

# If your backend is on a different domain than the frontend,
# you might need to enable CORS on the backend

# For local development, you can use the proxy in package.json instead
# and leave this commented out
# REACT_APP_API_URL=http://localhost:8000

# Refresh interval in milliseconds (5 minutes)
REACT_APP_REFRESH_INTERVAL=300000

# Default time range in days
REACT_APP_DEFAULT_TIME_RANGE=30 